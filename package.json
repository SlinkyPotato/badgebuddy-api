{
  "name": "badge-buddy-api",
  "version": "0.0.6-22",
  "description": "Microservices API for Badge Buddy",
  "author": "SlinkyPotato <patinobrian@gmail.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "20.11.1",
    "pnpm": "8.15.5"
  },
  "engineStrict": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "build": "nest build",
    "build:docker": "docker compose -f compose.local.yml build ",
    "clean": "rm -rf dist",
    "clean:dep": "rm -rf dist && rm -rf node_modules",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:docker": "docker compose up --build",
    "dev:debug": "nest start --debug --watch",
    "dev:api": "cd ../badge-buddy-api && pnpm dev",
    "dev:bot": "cd ../badge-buddy-bot && pnpm dev",
    "dev:processor": "cd ../badge-buddy-processor && pnpm dev",
    "dev:web": "cd ../badge-buddy-web && pnpm dev",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "env:push": "dotenv-vault push",
    "env:open": "dotenv-vault open",
    "env:pull": "dotenv-vault pull",
    "publish-docker:staging": "node publish.docker.js staging latest-beta",
    "publish-docker:production": "node publish.docker.js production latest",
    "sync": "git fetch && git pull && pnpm install",
    "sync:env": "pnpm sync && dotenv-vault pull"
  },
  "dependencies": {
    "@badgebuddy/common": "2.3.0-10",
    "@discord-nestjs/common": "^5.2.12",
    "@discord-nestjs/core": "^5.3.14",
    "@fastify/static": "^6.12.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-fastify": "^10.3.9",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/qs": "^6.9.15",
    "axios": "^1.7.2",
    "base64url": "^3.0.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.9",
    "cache-manager": "^5.6.1",
    "cache-manager-redis-yet": "^4.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "dotenv-vault": "^1.26.1",
    "google-auth-library": "^9.10.0",
    "joi": "^17.13.1",
    "mjml": "^4.15.3",
    "mysql2": "^3.10.0",
    "nodemailer": "^6.9.13",
    "qs": "^6.12.1",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.5.25",
    "@types/bcrypt": "^5.0.2",
    "@types/mjml": "^4.7.4",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
