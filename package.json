{
  "name": "badge-buddy-api",
  "version": "0.0.6-12",
  "description": "Microservices API for Badge Buddy",
  "author": "SlinkyPotato <patinobrian@gmail.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "20.9.0",
    "pnpm": "8.10.5"
  },
  "engineStrict": true,
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "build": "nest build",
    "build:docker": "docker compose -f compose.local.yml build ",
    "clean": "rm -rf dist",
    "clean:dep": "rm -rf dist && rm -rf node_modules",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:docker": "docker compose -f compose.local.yml up",
    "dev:debug": "nest start --debug --watch",
    "dev:api": "cd ../badge-buddy-api && pnpm dev",
    "dev:bot": "cd ../badge-buddy-bot && pnpm dev",
    "dev:processor": "cd ../badge-buddy-processor && pnpm dev",
    "dev:web": "cd ../badge-buddy-web && pnpm dev",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "env:push": "dotenv-vault push",
    "env:open": "dotenv-vault open",
    "env:pull": "dotenv-vault pull",
    "publish-docker:staging": "node publish.docker.js staging latest-beta",
    "publish-docker:production": "node publish.docker.js production latest",
    "sync": "git fetch && git pull && dotenv-vault pull"
  },
  "dependencies": {
    "@badgebuddy/common": "2.0.0-23",
    "@discord-nestjs/common": "^5.2.10",
    "@discord-nestjs/core": "^5.3.12",
    "@fastify/static": "^6.12.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-fastify": "^10.2.10",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "@types/qs": "^6.9.10",
    "axios": "^1.6.2",
    "base64url": "^3.0.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "cache-manager": "^5.3.1",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "dotenv-vault": "^1.25.0",
    "google-auth-library": "^9.4.1",
    "joi": "^17.11.0",
    "mjml": "^4.14.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "qs": "^6.11.2",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/bcrypt": "^5.0.2",
    "@types/mjml": "^4.7.4",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
