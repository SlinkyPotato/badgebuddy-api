{
  "name": "badge-buddy-api",
  "version": "0.0.6-8",
  "description": "Microservices API for Badge Buddy",
  "author": "SlinkyPotato <patinobrian@gmail.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "engineStrict": true,
  "packageManager": "pnpm@8.x",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "rm -rf dist",
    "clean:dep": "rm -rf dist && rm -rf node_modules",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "dev:api": "cd ../badge-buddy-api && pnpm dev",
    "dev:bot": "cd ../badge-buddy-bot && pnpm dev",
    "dev:processor": "cd ../badge-buddy-processor && pnpm dev",
    "dev:web": "cd ../badge-buddy-web && pnpm dev",
    "start:prod": "node dist/main",
    "build:docker": "docker compose --profile local up --build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "env:push": "dotenv-vault push",
    "env:open": "dotenv-vault open",
    "env:pull": "dotenv-vault pull",
    "publish-docker:staging": "node publish.docker.js staging latest-beta",
    "publish-docker:production": "node publish.docker.js production latest"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.8",
    "@discord-nestjs/core": "^5.3.10",
    "@fastify/static": "^6.11.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.4",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.2.4",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-fastify": "^10.2.4",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/terminus": "^10.0.1",
    "@solidchain/badge-buddy-common": "1.1.0-0",
    "axios": "^1.5.0",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "dotenv-vault": "^1.25.0",
    "joi": "^17.10.1",
    "mongoose": "^7.5.0",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "standard-version": "^9.5.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.4",
    "@types/node": "^20.5.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.4",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
